{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "blog_categories",
  "info": {
    "singularName": "blog-category",
    "pluralName": "blog-categories",
    "displayName": "Blog-category",
    "name": "blog-category"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "blogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "mappedBy": "blog_categories"
    }
  }
}
