{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "name": "blog"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "author": {
      "type": "string"
    },
    "date": {
      "type": "string"
    },
    "comments": {
      "type": "integer",
      "default": 0
    },
    "blog_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-category.blog-category",
      "inversedBy": "blogs"
    },
    "content": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false
    },
    "type": {
      "type": "string"
    },
    "demo": {
      "type": "string"
    },
    "page": {
      "type": "string"
    }
  }
}
